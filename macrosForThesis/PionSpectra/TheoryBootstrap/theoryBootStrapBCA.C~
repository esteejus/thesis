void loadVec(vector< vector<double>> &d, TGraphErrors *g)
{
  int num_bins = g->GetN();
   for(int iBin = 0; iBin < num_bins; iBin++)
    {
      double x,y;
      g->GetPoint(iBin,x,y);
      d.at(iBin).push_back(y);
    }
}


double BootStrap(vector< vector<double>> data)
{

  if(data.size() == 0)
    {
      cout<<"Data vec is EMPTY!!! "<<endl;
      return -1;
    }

  vector<double> boot(data.at(0).size(),0);

  for(int iTheory = 0; iTheory < data.size(); iTheory++)
    {
      for(int iBin = 0; iBin < data.at(iTheory).size(); iBin++)
	{


	}
    }

}



void theoryBootStrapBCA()
{

    TFile *pBUU_soft_no_f = TFile::Open("/home/justin/Homework/pBUU/K210EM100/input_for_esym.info/pBUUSoft_r-1.root");
